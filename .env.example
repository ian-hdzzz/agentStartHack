# ============================================
# AquaHub Agent Server - Environment Variables
# ============================================

# OpenAI API Key (required)
OPENAI_API_KEY=your-openai-api-key-here

# Server port (optional, defaults to 3000)
PORT=3000

# AquaHub API URL (required - the FastAPI backend)
AQUAHUB_API_URL=http://localhost:8000

# Evolution API (for WhatsApp integration, optional)
EVOLUTION_API_URL=https://evolution.whoopflow.com
EVOLUTION_API_KEY=your_evolution_api_key
